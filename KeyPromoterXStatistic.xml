<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug...">
        <Item>
          <IdeaActionID>ChooseDebugConfiguration</IdeaActionID>
          <ShortCut>'⌃⌥D'</ShortCut>
          <Description>Debug...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F12'</ShortCut>
          <Description>Declaration</Description>
          <Count>21</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Maximize Tool Window">
        <Item>
          <IdeaActionID>MaximizeToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘''</ShortCut>
          <Description>Maximize Tool Window</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="New...">
        <Item>
          <IdeaActionID>NewElement</IdeaActionID>
          <ShortCut>'⌘N'</ShortCut>
          <Description>New...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⇧⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste from History...">
        <Item>
          <IdeaActionID>PasteMultiple</IdeaActionID>
          <ShortCut>'⌘K, ⌘V' or '⌘K, V'</ShortCut>
          <Description>Paste from History...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'⌥⌘L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore tool window size">
        <Item>
          <IdeaActionID>MaximizeToolWindow</IdeaActionID>
          <ShortCut>'⇧⌘''</ShortCut>
          <Description>Restore tool window size</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run Gulp/Grunt/npm Task">
        <Item>
          <IdeaActionID>RunJsbtTask</IdeaActionID>
          <ShortCut>'⌥F11'</ShortCut>
          <Description>Run Gulp/Grunt/npm Task</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Save As...">
        <Item>
          <IdeaActionID>SavaAs</IdeaActionID>
          <ShortCut>'⇧⌘S'</ShortCut>
          <Description>Save As...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>